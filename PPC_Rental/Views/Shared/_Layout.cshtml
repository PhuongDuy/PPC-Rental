
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <link href="~/Content/StyleSearch.css" rel="stylesheet" />

    @Styles.Render("~/Content/css")
